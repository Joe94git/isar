Signed-off-by: Lau Wan Yee <wan.yee.lau@intel.com>
---
 arch/Kconfig | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/Kconfig b/arch/Kconfig
index 632d60e13494..e79f260230f6 100644
--- a/arch/Kconfig
+++ b/arch/Kconfig
@@ -901,7 +901,7 @@ config ARCH_NO_PREEMPT
 	bool
 
 config ARCH_SUPPORTS_RT
